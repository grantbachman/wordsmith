<%= link_to "New Quiz", new_quiz_path, class: 'btn btn-success' %>

<div id="quiz_calendar" class="centerDiv">
	<%= content_tag 'div', id: 'quiz_data', data: { quizzes: @quizzes } {} %> 
	<div id="calendar_nav">
		<span class="last"><%= link_to "Last Month", '#', id: 'last_month' %></span>
		<span class="title"></span>
		<span class="next"><%= link_to "Next Month", '#', id: 'next_month' %></span>
	</div>	
	<table id='calendar'>
		<tr>
			<% day = ['Sun','Mon','Tue','Wed','Thu','Fri','Sat'] %>
			<% day.count.times do |index| %>
				<th><%= day[index] %></th> 
			<% end %>
		</tr>
	</table>	
</div>
